// -------------read---------------------
tracepoint:syscalls:sys_enter_read /comm==""/ 
{ 
    @read_buf[tid] = args->buf;
    @read_count[tid] = args->count;
    @read_fd[tid] = args->fd;
} 
tracepoint:syscalls:sys_exit_read /comm==""/
{
    printf("read %d %d %s", @read_fd[tid], args->ret, str(@read_buf[tid], @read_count[tid]));
    clear(@read_buf);
    clear(@read_count);
    clear(@read_fd);
}
    
